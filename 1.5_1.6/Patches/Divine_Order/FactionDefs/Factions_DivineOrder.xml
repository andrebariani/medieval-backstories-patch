<?xml version="1.0" encoding="utf-8"?>
<Patch>

  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Divine Order</li>
    </mods>
    <match Class="PatchOperationSequence">
      <!-- <success>Always</success> -->
      <operations>
        <li Class="PatchOperationReplace">
          <xpath>Defs/FactionDef[defName="BotchJob_DivineOrderFaction"]/backstoryFilters/li</xpath>
          <value>
            <li>
							<categories>
								<li>Medieval</li>
								<li>MedievalCommon</li>
							</categories>
						</li>
          </value>
        </li>

        <li Class="PatchOperationReplace">
          <xpath>Defs/FactionDef[defName="BotchJob_DivineOrderPlayerFaction"]/backstoryFilters/li</xpath>
          <value>
            <li>
							<categories>
                <li>Medieval</li>
                <li>MedievalCommon</li>
                <li>MedievalKnight</li>
              </categories>
            </li>
          </value>
        </li>
      </operations>
    </match>
  </Operation>

</Patch>