<?xml version="1.0" encoding="utf-8"?>
<Patch>

  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Vanilla Factions Expanded - Empire</li>
    </mods>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationReplace">
          <xpath>/Defs/FactionDef[defName="VFEE_NewFamily"]/backstoryFilters/li</xpath>
          <value>
            <li>
              <categories>
                <li>Medieval</li>
                <li>MedievalCommon</li>
              </categories>
            </li>
          </value>
        </li>

        <li Class="PatchOperationReplace">
          <xpath>/Defs/FactionDef[defName="VFEE_Deserters"]/backstoryFilters/li</xpath>
          <value>
            <li>
              <categories>
                <li>Medieval</li>
                <li>MedievalCommon</li>
              </categories>
            </li>
          </value>
        </li>
      </operations>
    </match>
  </Operation>

</Patch>