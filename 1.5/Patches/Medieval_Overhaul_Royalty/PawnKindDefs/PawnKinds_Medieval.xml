<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Medieval Overhaul: Royalty</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<li Class="PatchOperationReplace">
					<xpath>
						Defs/PawnKindDef[@Name="Accurex_EmpireCommonerBase"]/backstoryFilters/li/categories/li</xpath>
					<value>
						<li>MedievalCommon</li>
					</value>
				</li>

				<li Class="PatchOperationReplace">
					<xpath>
						Defs/PawnKindDef[@Name="Accurex_EmpireFigtherBase"]/backstoryFilters/li/categories/li</xpath>
					<value>
						<li>MedievalKnight</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>

</Patch>