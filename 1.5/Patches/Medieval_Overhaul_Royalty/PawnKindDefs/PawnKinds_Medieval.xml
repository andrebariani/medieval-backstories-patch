<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Medieval Overhaul: Royalty</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<li Class="PatchOperationReplace">
					<xpath>
						/Defs/PawnKindDef[@Name="Accurex_EmpireCommonerBase"]/backstoryCategories/li[text()="ImperialCommon"]</xpath>
					<value>
						<li>
							<categories>
								<li>Medieval</li>
								<li>MedievalCommon</li>
								<li>imperialCommon</li>
							</categories>
						</li>
					</value>
				</li>

				<li Class="PatchOperationReplace">
					<xpath>
						/Defs/PawnKindDef[@Name="Accurex_EmpireFigtherBase"]/backstoryCategories/li[text()="ImperialFighter"]</xpath>
					<value>
						<li>
							<categories>
								<li>Medieval</li>
								<li>MedievalKnight</li>
								<li>ImperialFighter</li>
							</categories>
						</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>

</Patch>