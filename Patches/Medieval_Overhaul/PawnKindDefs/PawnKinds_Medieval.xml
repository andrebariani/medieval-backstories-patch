<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Medieval Overhaul</li>
		</mods>
		<match Class="PatchOperationSequence">
			<success>Always</success>
			<operations>
				<li Class="PatchOperationReplace">
					<xpath>
						/Defs/PawnKindDef[@Name="DankPyon_MedievalPawnKindBase"]/backstoryCategories/li[text()="Tribal"]</xpath>
					<value>
						<li>Medieval</li>
						<li>MedievalCommon</li>
						<li>MedievalRoyal</li>
					</value>
				</li>

				<li Class="PatchOperationReplace">
					<xpath>
						/Defs/PawnKindDef[@Name="DankPyon_BrigandPawnKindBase"]/backstoryCategories/li[text()="Tribal"]</xpath>
					<value>
						<li>Medieval</li>
						<li>MedievalCommon</li>
						<li>MedievalRoyal</li>
					</value>
				</li>

				<li Class="PatchOperationReplace">
					<xpath>
						/Defs/PawnKindDef[@Name="DankPyon_Medieval_Knight"]/backstoryCategories/li[text()="Medieval"]</xpath>
					<value>
						<li>MedievalKnight</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>

</Patch>