<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Medieval Overhaul</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/FactionDef[defName="DankPyon_PlayerKingdom"]/backstoryFilters/li</xpath>
					<value>
						<li>
							<categories>
								<li>Medieval</li>
								<li>MedievalCommon</li>
							</categories>
						</li>
					</value>
				</li>

				<li Class="PatchOperationReplace">
					<xpath>/Defs/FactionDef[defName="DankPyon_LoneWolf"]/backstoryFilters/li</xpath>
					<value>
						<li>
							<categories>
								<li>MedievalKnight</li>
							</categories>
						</li>
					</value>
				</li>

				<li Class="PatchOperationReplace">
					<xpath>/Defs/FactionDef[defName="DankPyon_MercenaryCompany"]/backstoryFilters/li</xpath>
					<value>
						<li>
							<categories>
								<li>Medieval</li>
								<li>MedievalCommon</li>
							</categories>
						</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>

</Patch>