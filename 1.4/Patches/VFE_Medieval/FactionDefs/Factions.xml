<?xml version="1.0" encoding="utf-8"?>
<Patch>

    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Vanilla Factions Expanded - Medieval</li>
        </mods>
        <match Class="PatchOperationSequence">
            <success>Always</success>
            <operations>
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/FactionDef[@Name="VFEM_UnknownKingdomBase"]/backstoryFilters/li</xpath>
                    <value>
                        <li>
							<categories>
								<li>Medieval</li>
								<li>MedievalCommon</li>
							</categories>
						</li>
                    </value>
                </li>
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/FactionDef[@Name="VFEM_MedievalFactionBase"]/backstoryFilters/li</xpath>
                    <value>
                        <categories>
							<li>Medieval</li>
							<li>MedievalCommon</li>
						</categories>
                    </value>
                </li>
                <li Class="PatchOperationReplace">
                    <xpath>/Defs/FactionDef[defName="VFEM_PlayerKingdom"]/backstoryFilters/li</xpath>
                    <value>
                        <categories>
							<li>Medieval</li>
							<li>MedievalCommon</li>
						</categories>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>

</Patch>