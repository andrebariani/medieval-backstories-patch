<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>The Profaned</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationReplace">
					<xpath>Defs/FactionDef[defName="BotchJob_ProfanedFaction"]/backstoryFilters/li</xpath>
					<value>
						<li>
							<categories>
								<li>Tribal</li>
								<li>Medieval</li>
								<li>MedievalCommon</li>
								<li>MedievalKnight</li>
							</categories>
						</li>
					</value>
				</li>

				<li Class="PatchOperationReplace">
					<xpath>
						Defs/FactionDef[defName="BotchJob_ProfanedPlayerFaction"]/backstoryFilters/li</xpath>
					<value>
						<li>
							<categories>
								<li>Tribal</li>
								<li>Medieval</li>
								<li>MedievalCommon</li>
								<li>MedievalKnight</li>
							</categories>
						</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>

</Patch>